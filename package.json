{
  "name": "docs.syxbot",
  "version": "1.0.0",
  "description": "",
  "author": "Tom Rooman",
  "private": true,
  "scripts": {
    "eslint": "eslint ./",
    "start": "webpack-dev-server --open --mode development",
    "remove-modules": "rm -rf node_modules .yarn/build-state.yml .yarn/cache .yarn/install-state.gz .yarn/unplugged .pnp.js yarn.lock package-lock.json",
    "build": "rm -rf dist && yarn webpack --mode production",
    "cp:ssl:files": "rm -rf cert/ && mkdir cert/ && cp /etc/letsencrypt/live/syxbot.com/privkey.pem cert/ && cp /etc/letsencrypt/live/syxbot.com/fullchain.pem cert/",
    "start:container": "docker run --restart=always --network=host --name syxbot-website-nginx -d syxbot-website-nginx",
    "stop:container": "docker stop syxbot-website-nginx && docker container rm syxbot-website-nginx",
    "build:container": "docker image prune -f && yarn cp:master:config && yarn build && yarn cp:ssl:files && cp /etc/nginx/sites-enabled/default cert/ && docker build -t syxbot-website-nginx -f ./nginx/Dockerfile . && yarn cp:dev:config",
    "deploy:container": "yarn build:container && yarn tag:container && yarn push:container",
    "cp:master:config": "rm config.json && cp 'private/config - master.json' ./ && mv 'config - master.json' config.json",
    "cp:dev:config": "rm config.json && cp 'private/config - dev.json' ./ && mv 'config - dev.json' config.json",
    "tag:container": "docker tag syxbot-website-nginx syxbot.com:5000/syxbot-website-nginx",
    "push:container": "docker push syxbot.com:5000/syxbot-website-nginx"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@material-ui/core": "^4.11.0",
    "axios": "^0.20.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^4.3.0",
    "dateformat": "^3.0.3",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-import-resolver-node": "^0.3.4",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "pnp-webpack-plugin": "^1.6.4",
    "react": "^16.13.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.3",
    "reactstrap": "^8.6.0",
    "style-loader": "^2.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.2",
    "@types/lodash": "^4.14.162",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "eslint": "^7.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.1.1",
    "prop-types": "^15.7.2",
    "source-map-loader": "^1.1.1",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.1"
  }
}
