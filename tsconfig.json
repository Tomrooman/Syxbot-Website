{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "resolveJsonModule": true,
        "module": "commonjs",
        "lib": [
            "es2020",
            "dom"
        ],
        "suppressImplicitAnyIndexErrors": true,
        "target": "es5",
        "jsx": "react",
        "strict": true,
        "declaration": false,
        "esModuleInterop": true,
        "skipLibCheck": true
    }
}